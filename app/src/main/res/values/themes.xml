<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="JunNotes" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/design_default_color_error</item>
    </style>

    <style name="JunNotes.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="JunNotes.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="JunNotes.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>